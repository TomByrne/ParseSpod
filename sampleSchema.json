    "$arrayOps":{
		"add":{
			"method":"PUT",
			"post":"{\"@{field.name}\":{\"__op\":\"AddRelation\",\"objects\":[{\"__type\":\"Pointer\",\"className\":\"@{field.arrayType}\",\"objectId\":\"@{field.value.Id}\"}]}}"
		},
		"remove":{
			"method":"PUT",
			"post":"{\"@{field.name}\":{\"__op\":\"RemoveRelation\",\"objects\":[{\"__type\":\"Pointer\",\"className\":\"@{field.arrayType}\",\"objectId\":\"@{field.value.Id}\"}]}}"
		}
    },
    "$objectStaticOps":{
		"new":{
			"method":"GET"
		},
		"query":{
			"method":"GET",
			"args":[
				{
					"name":"where",
					"type":"parse.ParseQuery"
				}
			]
		}
    },
    "$objectOps":{
		"update":{
			"method":"GET"
		},
		"save":{
			"method":"PUT",
			"post":"@{object.changedProps.json}"
		},
		"remove":{
			"method":"DELETE",
			"post":"@{object.changedProps.json}"
		},
		"deleteField":{
			"method":"DELETE",
			"args":[
				{
					"name":"field",
					"type":"String"
				}
			],
			"post":"{\"@{args[0]}\":{\"__op\":\"Delete\"}}"
		}
    },

